<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.main.data_show.mapper.TaUserMapper">
    <select id="findAll" resultType="com.main.data_show.pojo.TaUser">
        select * from ta_user
    </select>

    <select id="findUserByUserName" resultType="com.main.data_show.pojo.TaUser">
        select * from ta_user where user_name = #{user_name}
    </select>

</mapper>